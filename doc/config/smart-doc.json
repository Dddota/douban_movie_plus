{
  "//": "服务器地址",
  "serverUrl": "https://localhost",
  "//": "是否开启严格模式,需要标准文档注释",
  "isStrict": false,
  "//": "是否将文档合并到一个文件",
  "allInOne": true,
  "//": "指定文档的输出路径",
  "outPath": "/home/humingk/gits/douban_movie_plus/doc/api",
  "//": "是否覆盖旧的文件(markdown)",
  "coverOld": true,
  "//": "controller包过滤，英文逗号隔开",
  "packageFilters": "",
  "//": "每个controller生成一个html文件时,生成html时加密文档名不暴露controller的名称",
  "md5EncryptedHtmlName": false,
  "//": "项目名称",
  "projectName": "豆瓣电影Plus RESTful-API文档",
  "//": "暂无用",
  "skipTransientField": true,
  "//": "是否显示接口作者名称",
  "showAuthor": true,
  "//": "数据字典 ?",
  "dataDictionaries": [
    {
      "//": "数据字典的名称",
      "title": "Code and Msg",
      "//": "数据字典枚举类名称",
      "enumClassName": "org.humingk.movie.common.enumeration.CodeAndMsg",
      "//": "数据字典字典码对应的字段名称",
      "codeField": "code",
      "//": "数据字典对象的描述信息字典",
      "descField": "msg"
    }
  ],
  "//": "设置文档变更记录",
  "revisionLogs": [
    {
      "//": "文档版本号",
      "version": "2.0.0",
      "//": "变更操作状态，eg：创建、更新等",
      "status": "create",
      "//": "文档变更作者",
      "author": "humingk",
      "//": "变更描述",
      "remarks": "创建RESTful-API文档"
    }
  ],
  "//": "自定义添加字段和注释，遇到同名字段则直接给相应字段加注释",
  "customResponseFields": [
    {
      "//": "覆盖响应码字段",
      "name": "code233",
      "//": "覆盖响应码的字段注释",
      "desc": "状态码233",
      "//": "设置响应码的值",
      "value": "0"
    }
  ],
  "//": "设置请求头",
  "requestHeaders": [
    {
      "name": "Authorization",
      "type": "string",
      "desc": "部分API需要认证访问",
      "required": false,
      "since": "-"
    }
  ],
  "//": "设置代码路径，默认会自动加载src/main/java",
  "sourceCodePaths": [
    {
      "path": "src/main/java",
      "desc": "default"
    }
  ]
}